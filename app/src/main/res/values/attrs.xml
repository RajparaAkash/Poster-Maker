<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="TemplateView" tools:ignore="ResourceName">
        <attr name="gnt_template_type" format="reference" />
        <attr name="gnt_template_min_height" format="integer" />
    </declare-styleable>

    <attr name="sweet_alert_content_text_color" format="reference|color"></attr>
    <attr name="sweet_alert_title_text_color" format="reference|color"></attr>
    <attr name="sweet_alert_bg_drawable" format="reference"></attr>

    <declare-styleable name="Poster_AutoStickerView"></declare-styleable>

    <declare-styleable name="Poster_Rotate_TV_Loading"></declare-styleable>

    <declare-styleable name="RatingBarAttributes">
        <attr name="numStars" format="integer" />
        <attr name="rating" format="float" />
        <attr name="starPadding" format="integer" />
        <attr name="drawableEmpty" format="reference" />
        <attr name="drawableFilled" format="reference" />
        <attr name="touchable" format="boolean" />
        <attr name="clearRatingEnabled" format="boolean" />
        <attr name="starWidth" format="dimension" />
        <attr name="starHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="Ad_AspectRatioLayout">
        <attr name="arl_widthRatio" format="float" />
        <attr name="arl_heightRatio" format="float" />
    </declare-styleable>

</resources>
